                                 ; Constants
                                 .equ DOT_MASK = 0x80
                                 .equ OUT_TO_FIRST_DIG = 0x10
                                 .equ OUT_TO_SECOND_DIG = 0x20
                                 .equ OUT_TO_THIRD_DIG = 0x40
                                 .equ TIMER0_COUNTER = 173
                                 .equ NEED_DOT_ON_FIRST_DIGIT = 0
                                 .equ NEED_DOT_ON_SECOND_DIGIT = 1
