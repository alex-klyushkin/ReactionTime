                                 .MACRO SETUP_TIMER1_COMPARE
                                 	ldi @0, HIGH(@2)
                                 	ldi @1, LOW(@2)
                                 	out OCR1AH, @0
                                 	out OCR1AL, @1
                                 .ENDMACRO
                                 
                                 
                                 
                                 .MACRO ENABLE_TIMER_INTERRUPT
                                 	in @0, TIMSK
                                 	sbr @0, (1 << @1)
                                 	out TIMSK, @0
                                 .ENDMACRO 
                                 
                                 
                                 
                                 .MACRO DISABLE_TIMER_INTERRUPT
                                 	in @0, TIMSK
                                 	cbr @0, (1 << @1)
                                 	out TIMSK, @0
                                 .ENDMACRO 
                                 
                                 
                                 
                                 .MACRO UPDATE_DIGIT
                                 	ldi ZH, HIGH(digitToMask * 2)
                                 	ldi ZL, LOW(digitToMask * 2)
                                 	add ZL, @0
                                 	clr @0
                                 	adc ZH, @0
                                 	lpm
                                 	mov @1, r0
                                 .ENDMACRO
                                 
                                 
                                 
